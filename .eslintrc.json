{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "CONFIG": false,
        "_": false,
        "s": false,
        "angular": false
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            2,
            4
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}